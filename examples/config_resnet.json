{
    "info": {
        "app_name": "image_classification",
        "framework": "TF"
    },
    "data": {
        "training_dir": "../../../data/lr_imagenet/train/",
        "validation_dir": "../../../data/lr_imagenet/validation/",
        "input_file_format": "npz",

		"params": {
			"data_generator": "ImageDataHandler",
			"dtype": "int64"
		}
